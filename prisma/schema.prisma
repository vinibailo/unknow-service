generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model File {
  id        String   @id @default(cuid())
  key       String   @unique
  name      String
  size      Int
  createdAt DateTime @default(now())
  expiresAt DateTime
}
